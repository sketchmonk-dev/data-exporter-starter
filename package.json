{
  "name": "empty",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "tsc --build",
    "build.watch": "tsc --build --watch",
    "lint": "eslint .",
    "start": "ts-node src/worker.ts",
    "dev": "nodemon src/worker.ts",
    "workflow": "ts-node src/client.ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "test": "mocha --exit --require ts-node/register --require source-map-support/register src/mocha/*.test.ts"
  },
  "nodemonConfig": {
    "execMap": {
      "ts": "ts-node"
    },
    "ext": "ts",
    "watch": [
      "src"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.563.0",
    "@aws-sdk/lib-storage": "^3.563.0",
    "@aws-sdk/s3-request-presigner": "^3.563.0",
    "@novu/node": "^0.24.1",
    "@temporalio/activity": "^1.9.0",
    "@temporalio/client": "^1.9.0",
    "@temporalio/worker": "^1.9.0",
    "@temporalio/workflow": "^1.9.0",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "install": "^0.13.0",
    "nanoid": "3.x",
    "nodemailer": "^6.9.13",
    "npm": "^10.6.0",
    "radash": "^11.0.0",
    "zod": "^3.23.4",
    "@fast-csv/format": "^4.3.5",
    "mongodb": "^6.5.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@temporalio/testing": "^1.9.0",
    "@tsconfig/node18": "^1.0.0",
    "@types/mocha": "8.x",
    "@types/node": "^16.11.43",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-deprecation": "^1.2.1",
    "mocha": "8.x",
    "nodemon": "^2.0.12",
    "prettier": "^2.8.8",
    "ts-node": "^10.8.1",
    "typescript": "^4.4.2"
  }
}
